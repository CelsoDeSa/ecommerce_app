<div class="container center-block">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Foto:</strong>
    <%= @produto.foto %>
  </p>

  <p>
    <strong>Descrição:</strong>
    <%= @produto.descricao %>
  </p>

  <p>
    <strong>Preço:</strong>
    <%= @produto.preco %>
  </p>

  <%= link_to 'Editar', edit_produto_path(@produto) %> |
  <%= link_to 'Voltar', produtos_path %>
</div>
